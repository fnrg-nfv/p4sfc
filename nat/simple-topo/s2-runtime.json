{
  "target": "bmv2",
  "p4info": "build/nf.p4.p4info.txt",
  "bmv2_json": "build/nf.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:01:00",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["188.188.188.0", 24]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:00",
        "port": 2
      }
    },
    {
      "table": "MyIngress.nat_exact",
      "default_action": true,
      "action_name": "MyIngress.send_to_server",
      "action_params": { 
        "serverPort": 3
      }
    }
  ]
}
